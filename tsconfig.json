{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@items": [
        "src/data/items.json"
      ],
      "@itemTypes": [
        "src/data/itemTypes.json"
      ],
      "@actions": [
        "src/data/actions.json"
      ],
      "@recipeCategories": [
        "src/data/recipeCategories.json"
      ],
      "@recipes": [
        "src/data/recipes.json"
      ],
      "@resources": [
        "src/data/resources.json"
      ],
      "@recipeIngredients": [
        "src/data/recipeIngredients.json"
      ],
      "@recipeResults": [
        "src/data/recipeResults.json"
      ],
      "@jobsItems": [
        "src/data/jobsItems.json"
      ],
      "@equipmentItemTypes": [
        "src/data/equipmentItemTypes.json"
      ],
      "@states": [
        "src/data/states.json"
      ],
      "@dungeons": [
        "src/data/dungeons.json"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}